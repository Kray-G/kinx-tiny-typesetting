namespace Typesetting {

    namespace Command {

        class Copyright(info_, context_) {
            public exec(params) {
                var w = new Typesetting.Word(info_).setWord(context_.font.fontObj, context_.font.fontSize, '\u00A9');
                w.noGlue = true;
                w.noPrevGlue = true;
                return w;
            }
        }

    } # namespace Command

} # namespace Typesetting
